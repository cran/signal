citHeader("To cite signal in publications use:")

citEntry(entry       = "Manual",
    title        = "{signal}: Signal processing",
    author       = person("signal developers"),
    year         = 2011,
    url          = "http://r-forge.r-project.org/projects/signal/",
    textVersion  = paste("signal developers (2011).",
        "signal: Signal processing.",
        "URL: http://r-forge.r-project.org/projects/signal/.")
)
